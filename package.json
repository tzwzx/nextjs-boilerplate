{
  "name": "nextjs-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "biome check",
    "format": "biome check --write",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "stylelint": "stylelint 'src/**/*.scss'",
    "stylelint:fix": "stylelint 'src/**/*.scss' --fix",
    "tsc": "tsgo --noEmit",
    "package-format": "syncpack format",
    "package-lint-semver-ranges": "syncpack lint-semver-ranges",
    "knip": "knip",
    "test:unit": "bun test tests/unit",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "check-all": "bun scripts/check-all.ts",
    "check-all:fix": "bun scripts/check-all.ts fix",
    "postinstall": "lefthook install"
  },
  "dependencies": {
    "next": "16.0.10",
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.9",
    "@happy-dom/global-registrator": "20.0.11",
    "@playwright/test": "1.57.0",
    "@testing-library/react": "16.3.1",
    "@types/bun": "1.3.4",
    "@types/node": "25.0.2",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@typescript/native-preview": "7.0.0-dev.20251215.1",
    "babel-plugin-react-compiler": "1.0.0",
    "concurrently": "9.2.1",
    "knip": "5.74.0",
    "lefthook": "2.0.12",
    "prettier": "3.7.4",
    "sass": "1.97.0",
    "stylelint": "16.26.1",
    "stylelint-config-recess-order": "7.4.0",
    "stylelint-config-recommended-scss": "16.0.2",
    "stylelint-prettier": "5.0.3",
    "syncpack": "13.0.4",
    "typescript": "5.9.3",
    "ultracite": "6.4.1",
    "yaml": "2.8.2"
  },
  "engines": {
    "node": "24.x",
    "bun": "1.x"
  }
}
