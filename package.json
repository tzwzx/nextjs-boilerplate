{
  "name": "nextjs-boilerplate",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "biome check",
    "format": "biome check --write",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "stylelint": "stylelint 'src/**/*.scss'",
    "stylelint:fix": "stylelint 'src/**/*.scss' --fix",
    "tsc": "tsgo --noEmit",
    "package-format": "syncpack format",
    "package-lint-semver-ranges": "syncpack lint-semver-ranges",
    "knip": "knip",
    "test:unit": "bun test tests/unit",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "check-all": "bun scripts/check-all.ts",
    "check-all:fix": "bun scripts/check-all.ts fix",
    "postinstall": "lefthook install"
  },
  "dependencies": {
    "next": "15.5.5",
    "react": "19.2.0",
    "react-dom": "19.2.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.6",
    "@happy-dom/global-registrator": "20.0.1",
    "@playwright/test": "1.56.0",
    "@testing-library/react": "16.3.0",
    "@tsconfig/next": "2.0.3",
    "@types/bun": "1.3.0",
    "@types/node": "24.7.2",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "@typescript/native-preview": "7.0.0-dev.20251015.1",
    "babel-plugin-react-compiler": "19.1.0-rc.1-rc-af1b7da-20250421",
    "concurrently": "9.2.1",
    "knip": "5.65.0",
    "lefthook": "1.13.6",
    "prettier": "3.6.2",
    "sass": "1.93.2",
    "stylelint": "16.25.0",
    "stylelint-config-recess-order": "7.3.0",
    "stylelint-config-recommended-scss": "16.0.2",
    "stylelint-prettier": "5.0.3",
    "syncpack": "13.0.4",
    "typescript": "5.9.3",
    "ultracite": "5.6.4",
    "yaml": "2.8.1"
  },
  "engines": {
    "node": "22.x",
    "bun": "1.x"
  }
}
